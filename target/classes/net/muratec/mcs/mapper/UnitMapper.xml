  <!-- Unit一覧取得  -->
  <select id="selectUnitRowNum" parameterType="net.muratec.mcs.entity.common.AjaxDataTablesReqBaseEntity" resultType="net.muratec.mcs.model.UnitModel">
    select
      UNIT_ID,
      AMHS_ID,
      UNIT_NAME,
      MODULE_ID,
      MODULE_NAME,
      AVAILABLE,
      UNIT_L_STATE,
      UNIT_L_STATE_SET_TIME,
      UNIT_L_STATE_SET_AUTHOR
    from(
      select
        row_number() over(order by U.UNIT_ID asc) as RN,
        U.UNIT_ID,
        U.AMHS_ID,
        U.UNIT_NAME,
        M.MODULE_ID,
        M.MODULE_NAME,
        (select STRING from STRING_MASTER where KEY = 'AVAIL_STRING' and CODE = M.AVAILABLE) as AVAILABLE,
        (select STRING from STRING_MASTER where KEY = 'UNIT_LOGICAL_STATE_STRING' and CODE = U.UNIT_L_STATE) as UNIT_L_STATE,
        U.UNIT_L_STATE_SET_TIME,
        U.UNIT_L_STATE_SET_AUTHOR
      from
        unit u
        left outer join module m
          on U.MODULE_ID = M.MODULE_ID
      order by UNIT_ID asc
    )
    where rn between #{fromRecordNum,jdbcType=DECIMAL} and #{toRecordNum,jdbcType=DECIMAL}
  </select>

